# Find out about expected output.
{ set +x
$as_echo "$at_srcdir/localization.at:44: (LANG=de_DE LANGUAGE=de_DE LC_ALL=de_DE \$CC \$CPPFLAGS \$CFLAGS -c a.c) || exit 1"
