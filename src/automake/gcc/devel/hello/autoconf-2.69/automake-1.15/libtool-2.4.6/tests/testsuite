at_fn_check_prepare_dynamic "$LIBTOOL --mode=link $CC $CFLAGS $LDFLAGS 	   -o main main.lo libbar.la" "darwin.at:186"
( $at_check_trace; $LIBTOOL --mode=link $CC $CFLAGS $LDFLAGS 	   -o main main.lo libbar.la
