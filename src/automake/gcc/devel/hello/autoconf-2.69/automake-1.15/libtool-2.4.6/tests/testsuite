echo stdout:; cat "$at_stdout"
at_fn_check_status 0 $at_status "$at_srcdir/f77demo.at:334"
if $at_failed; then :
  { set +x
$as_echo "$at_srcdir/f77demo.at:334: \$GREP 'require .*but have' stderr && (exit 77)"
