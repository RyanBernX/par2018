$as_echo "$at_srcdir/demo.at:388: \$AUTOCONF "
at_fn_check_prepare_dynamic "$AUTOCONF " "demo.at:388"
( $at_check_trace; $AUTOCONF
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
echo stderr:; cat "$at_stderr"
