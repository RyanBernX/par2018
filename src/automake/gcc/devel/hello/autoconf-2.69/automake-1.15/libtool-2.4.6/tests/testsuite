$at_failed && at_fn_log_failure
$at_traceon; }


for target in all
do
  { set +x
$as_echo "$at_srcdir/old-m4-iface.at:91: \$as_unset LIBTOOL; \$as_unset LIBTOOLIZE; \$MAKE \$target "
