( $at_check_trace; if "$lt_exe" ; then :; else lt_status=$?; 	   test "$lt_status" != 0 && 	   test "X$host" != "X$build" && test -x "$lt_exe" && exit 77; 	   exit $lt_status; fi
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
