m4_ifndef([AC_CONFIG_MACRO_DIRS],
	  [m4_define([AC_CONFIG_MACRO_DIRS], m4_defn([AC_CONFIG_MACRO_DIR]))])
_ATEOF

echo "$ACLOCAL -I m4$macro_dir"; { set +x
